{
  "name": "pixelos-kernel-terminal",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron-main.cjs",
  "scripts": {
    "dev": "vite",
    "dev:electron": "ELECTRON_DEV_SERVER_URL=http://localhost:5173 VITE_DEV_SERVER_URL=http://localhost:5173 concurrently \"vite --port 5173\" \"wait-on http://localhost:5173 && ELECTRON_DISABLE_SANDBOX=1 electron --no-sandbox .\"",
    "preview:electron": "ELECTRON_DEV_SERVER_URL=http://localhost:4173 VITE_DEV_SERVER_URL=http://localhost:4173 concurrently \"vite preview --host --port 4173\" \"wait-on http://localhost:4173 && ELECTRON_DISABLE_SANDBOX=1 electron --no-sandbox .\"",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "lucide-react": "^0.554.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@webgpu/types": "^0.1.66",
    "concurrently": "^9.1.0",
    "electron": "^33.2.0",
    "wait-on": "^8.0.2",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
